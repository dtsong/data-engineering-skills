{{PROJECT_NAME}}:
  target: dev
  outputs:
    dev:
      type: sqlserver
      driver: "ODBC Driver 18 for SQL Server"
      server: "{{ env_var('SQLSERVER_HOST') }}"
      port: 1433
      database: "{{SQLSERVER_DATABASE}}"
      schema: "{{SQLSERVER_SCHEMA}}"
      user: "{{ env_var('SQLSERVER_USER') }}"
      password: "{{ env_var('SQLSERVER_PASSWORD') }}"
      threads: 4
      trust_cert: true
