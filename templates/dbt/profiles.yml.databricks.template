{{PROJECT_NAME}}:
  target: dev
  outputs:
    dev:
      type: databricks
      catalog: "{{DATABRICKS_CATALOG}}"
      schema: "{{DATABRICKS_SCHEMA}}"
      host: "{{ env_var('DATABRICKS_HOST') }}"
      http_path: "{{ env_var('DATABRICKS_HTTP_PATH') }}"
      token: "{{ env_var('DATABRICKS_TOKEN') }}"
      threads: 4
